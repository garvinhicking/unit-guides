
<div class="toc-header">
    <div class="toc-title">
        {% set pagerLinks = getPagerLinks() %}
        {%- for link in pagerLinks -%}
            {%- if link.relation=='top' -%}
                <a class="toc-title-project" href="{{ link.url }}">{{ env.projectNode.title }}</a>
            {% endif -%}
        {%- endfor -%}

        {% if env.projectNode.version -%}
            <div class="toc-version-wrapper" id="toc-version-wrapper">
                <!-- no tookies for you -->
            </div>
        {%- endif -%}
    </div>
    <div class="toc-actions">
        <label class="toc-toggle" for="toggleToc">
            Menu
        </label>
    </div>
</div>
